{
  "name": "ai-header-commenter",
  "displayName": "AI Header Commenter",
  "description": "Generates AI-powered header comments for code files using custom REST API",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:aiHeaderCommenter.callAIDocumentor",
    "onCommand:aiHeaderCommenter.generateHeaderForCurrentFile"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aiHeaderCommenter.callAIDocumentor",
        "title": "Call AI Documentor (Workspace)"
      },
      {
        "command": "aiHeaderCommenter.generateHeaderForCurrentFile",
        "title": "Generate AI Header for Current File"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}
