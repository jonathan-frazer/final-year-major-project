{
  "name": "ai-header-commenter",
  "displayName": "AI Header Commenter",
  "description": "Generates AI-powered header comments for code files using custom REST API",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:aiHeaderCommenter.generateAIDocumentationWorkspace",
    "onCommand:aiHeaderCommenter.generateAIDocumentationCurrentFile",
    "onCommand:aiHeaderCommenter.generateAIDocumentationCurrentFolder",
    "onCommand:aiHeaderCommenter.clearAIDocumentationWorkspace",
    "onCommand:aiHeaderCommenter.clearAIDocumentationCurrentFile",
    "onCommand:aiHeaderCommenter.clearAIDocumentationCurrentFolder"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aiHeaderCommenter.generateAIDocumentationWorkspace",
        "title": "Call AI Documentor (Entire Workspace)"
      },
      {
        "command": "aiHeaderCommenter.generateAIDocumentationCurrentFile",
        "title": "Call AI Documentor (Current File)"
      },
      {
        "command": "aiHeaderCommenter.generateAIDocumentationCurrentFolder",
        "title": "Call AI Documentor (Current Folder)"
      },
      {
        "command": "aiHeaderCommenter.clearAIDocumentationWorkspace",
        "title": "Clear AI Documentation (Entire Workspace)"
      },
      {
        "command": "aiHeaderCommenter.clearAIDocumentationCurrentFile",
        "title": "Clear AI Documentation (Current File)"
      },
      {
        "command": "aiHeaderCommenter.clearAIDocumentationCurrentFolder",
        "title": "Clear AI Documentation (Current Folder)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}
